{
  "__resolveType": "site/sections/Theme/Theme.tsx",
  "colorScheme": "any",
  "buttonStyle": {
    "--border-btn": "1px",
    "--rounded-btn": "0.2rem",
    "--btn-focus-scale": "0.9",
    "--animation-btn": "0.25s"
  },
  "otherStyles": {
    "--rounded-box": "1rem",
    "--rounded-badge": "1.9rem",
    "--animation-input": "0.2s",
    "--tab-border": "1px",
    "--tab-radius": "0.5rem"
  },
  "mode": "light",
  "mainColors": {
    "base-100": "#242424",
    "primary": "#e60000",
    "secondary": "#ededed",
    "tertiary": "#700000"
  },
  "font": {
    "__resolveType": "website/loaders/fonts/googleFonts.ts",
    "fonts": [
      {
        "variations": [
          {
            "weight": "400"
          },
          {
            "weight": "500"
          },
          {
            "weight": "600"
          },
          {
            "weight": "700"
          }
        ],
        "family": "roboto"
      }
    ]
  }
}